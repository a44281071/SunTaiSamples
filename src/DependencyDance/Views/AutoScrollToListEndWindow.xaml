<Window x:Class="DependencyDance.Views.AutoScrollToListEndWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:att="clr-namespace:DependencyDance"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:DependencyDance.Views"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="AutoScrollToListEndWindow"
        Width="300"
        Height="300"
        DataContext="{Binding RelativeSource={x:Static RelativeSource.Self}}"
        Loaded="Window_Loaded"
        Unloaded="Window_Unloaded"
        mc:Ignorable="d">
  <Grid>
    <ListBox att:ItemsControlAtt.IsScrollToEnd="True"
             ItemsSource="{Binding Datas}" />
  </Grid>
</Window>
